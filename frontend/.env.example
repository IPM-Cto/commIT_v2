# Auth0 Configuration
REACT_APP_AUTH0_DOMAIN=your-domain.auth0.com
REACT_APP_AUTH0_CLIENT_ID=your-auth0-client-id
REACT_APP_AUTH0_REDIRECT_URI=http://localhost:3000/callback
REACT_APP_AUTH0_AUDIENCE=https://api.commit.it

# API Configuration
REACT_APP_API_URL=http://localhost:8000/api

# App Configuration
REACT_APP_NAME=commIT
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development

# OpenAI Configuration (optional, if needed client-side)
REACT_APP_OPENAI_API_KEY=

# Google Maps API (optional, for location services)
REACT_APP_GOOGLE_MAPS_API_KEY=

# Feature Flags
REACT_APP_ENABLE_CHAT=true
REACT_APP_ENABLE_PAYMENTS=false
REACT_APP_ENABLE_ANALYTICS=false
